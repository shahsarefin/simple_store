<h1>Simple Store Products</h1>

<ul> 
    <% @products.each do |product| %>
        <li> 
        <strong>Title:</Strong> <%= product.title %><br>
        <strong>Price:</Strong> <%= product.price %><br>
        <strong>Stock Quantity:</Strong> <%= product.stock_quantity %>
        </li>
    <% end %>
</ul>
